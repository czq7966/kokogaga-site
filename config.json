{
	"version": "2.13",
	"configUrl": "config.json",
	"updateUrl": "",
	"autoUpdateConfig": false,
	"namespaces": {
		"nd": "amd/common/index.js",
		"promethean": "dist/server/amd/common/index.js",
		"mdm": "dist/server/amd/common/index.js",
		"test": "dist/server/amd/common/index.js",
		
		"admin": "dist/server/amd/admin/index.js",
		"__cert-watcher": "dist/server/amd/cert-watcher/index.js",
		"__cs-logger": null,		
		"signal-center": "amd/signal-center/index.js"
	},
	"websites": {
		"desktop": "/../desktop",
		"sdp-probe": "/../web/sdp-probe"
	},
	"http": [
		{
			"port": 2770
		},
		{
			"port": 8080
		}
	],
	"https": [
		{
			"port": 2771,
			"key": [
				"/etc/letsencrypt/live/adhoc-turn.ca.101.com/privkey.pem",
				"/etc/letsencrypt/live/mdm.hk.101.com/privkey.pem"
			],
			"cert": [
				"/etc/letsencrypt/live/adhoc-turn.ca.101.com/fullchain.pem",
				"/etc/letsencrypt/live/mdm.hk.101.com/fullchain.pem"
			]
		},
		{
			"port": 13670,
			"key": [
				"/etc/letsencrypt/live/adhoc-turn.ca.101.com/privkey.pem",
				"/etc/letsencrypt/live/mdm.hk.101.com/privkey.pem"
			],
			"cert": [
				"/etc/letsencrypt/live/adhoc-turn.ca.101.com/fullchain.pem",
				"/etc/letsencrypt/live/mdm.hk.101.com/fullchain.pem"
			]
		}
	],
	"clientConfig": {
		"rtcConfig": {
			"iceTransportPolicy": "all",
			"iceServers": [
				{
					"urls": [
						"stun:adhoc-turn.101.com:3478",
						"stun:adhoc-turn.101.com:3478?transport=udp"
					]
				},
				{
					"urls": [
						"turn:adhoc-turn.101.com:3478"
					],
					"username": "u1",
					"credential": "p1"
				}
			]
		}
	},
	"signalCenter": {
		"enabled": true,
		"signalerBase": "http://127.0.0.1:2770",
		"namespace": "signal-center"
	}
}