{
	"version": "2.13",
	"configUrl": "config.json",
	"updateUrl": "",
	"autoUpdateConfig": false,
	"namespaces": {
		"promethean": {
			"url": "amd/common/index.js",
			"type": "common",
			"useSignalCenter": true
		},
		"mdm": {
			"url": "amd/common/index.js",
			"type": "common",
			"useSignalCenter": true
		},	
		"nd": {
			"url": "amd/common/index.js",
			"type": "common",
			"useSignalCenter": true
		},			
		"test": {
			"url": "amd/common/index.js",
			"type": "common",
			"useSignalCenter": true
		},	
		"admin": {
			"url": "amd/admin/index.js",
			"type": "admin"
		},
		"certWatcher": {
			"url": "amd/cert-watcher/index.js",
			"type": "certWatcher"
		},
		"signalCenter": {
			"url": "amd/signal-center/index.js",
			"type": "signalCenter"
		},
		"signalClient": {			
			"url": "amd/signal-client/index.js",
			"type": "signalClient",
			"disabled": true
		},		
		"signalRedis": {
			"url": "amd/redis-signaler/index.js",
			"type": "signalRedis",
			"extra": {				
				"enabled": true,
				"url": "redis://127.0.0.1:6379",
				"handshakeInterval": 3000,
				"handshakeTimeout":  22000,
				"redundanceScript": "amd/redis-signaler/redundancy.lua"
			}
		}
	},
	"websites": {
		"desktop": "/../desktop",
		"sdp-probe": "/../web/sdp-probe"
	},
	"http": [
		{
			"port": 2770
		},
		{
			"port": 8080
		}
	],
	"https": [
		{
			"port": 2771,
			"key": [
				"/etc/letsencrypt/live/adhoc-turn.ca.101.com/privkey.pem",
				"/etc/letsencrypt/live/mdm.hk.101.com/privkey.pem"
			],
			"cert": [
				"/etc/letsencrypt/live/adhoc-turn.ca.101.com/fullchain.pem",
				"/etc/letsencrypt/live/mdm.hk.101.com/fullchain.pem"
			]
		},
		{
			"port": 13670,
			"key": [
				"/etc/letsencrypt/live/adhoc-turn.ca.101.com/privkey.pem",
				"/etc/letsencrypt/live/mdm.hk.101.com/privkey.pem"
			],
			"cert": [
				"/etc/letsencrypt/live/adhoc-turn.ca.101.com/fullchain.pem",
				"/etc/letsencrypt/live/mdm.hk.101.com/fullchain.pem"
			]
		}
	],
	"socketIOServer": {
		"path": "socket.io"
	},
	"clientConfig": {
		"rtcConfig": {
			"iceTransportPolicy": "all",
			"iceServers": [
				{
					"urls": [
						"stun:adhoc-turn.101.com:3478",
						"stun:adhoc-turn.101.com:3478?transport=udp"
					]
				},
				{
					"urls": [
						"turn:adhoc-turn.101.com:3478"
					],
					"username": "u1",
					"credential": "p1"
				}
			]
		}
	},
	"signalCenter": {
		"enabled": false,
		"signalerBase": "http://127.0.0.1:2770",
		"namespace": "signalCenter"
	}
}